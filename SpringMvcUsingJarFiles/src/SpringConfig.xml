<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   
   <bean id = "Demo" class = "com.org.SpringObjects.Demo">
      <property name = "message" value = "Hello World! project using spring mvc project"/>
   </bean>
     <bean id = "HelloConstr" class = "com.org.classess.HelloConstr">
      <constructor-arg value = "hello subhash"></constructor-arg>
      <property name="listOfString">
       <list>
       	<value>subhash</value>
       	<value>jsdafk</value>
       </list>
      </property>
     </bean>
     
        <bean id = "user" class = "com.test.user.User">
     		 <property name = "calc" ref = "CalculatorA"/>
     		 <property name = "name" value = "subhash"/>
   		</bean>
	   <bean id = "CalculatorA" class = "com.calc.springObjects.CalculatorA">
	   </bean>
</beans>